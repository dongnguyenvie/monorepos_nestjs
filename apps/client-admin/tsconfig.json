{
  "extends": "tsconfig/react-library.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ],
    "baseUrl": "./src",
    "lib": ["dom", "dom.iterable", "esnext"],
    "noImplicitAny": false,
    "resolveJsonModule": true,
  },
  "include": [
    "src"
  ]
}