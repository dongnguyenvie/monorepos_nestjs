{
  "name": "@noinghe/client-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.6.2",
    "@apollo/client": "3.3.16",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@handsontable/react": "4.0.0",
    "@reduxjs/toolkit": "1.3.2",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "25.1.4",
    "@types/node": "^16.11.14",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "5.0.15",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "5.0.1",
    "@types/testing-library__jest-dom": "5.0.2",
    "@types/webpack-env": "1.15.1",
    "antd": "4.16.8",
    "axios": "0.20.0",
    "connected-react-router": "6.8.0",
    "cross-env": "7.0.2",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "fontfaceobserver": "2.1.0",
    "graphql": "15.5.0",
    "handsontable": "8.0.0",
    "husky": "4.2.3",
    "i18next": "19.3.4",
    "i18next-browser-languagedetector": "4.0.2",
    "jest-styled-components": "7.0.2",
    "jodit": "3.20.4",
    "jwt-decode": "3.1.2",
    "lint-staged": "10.0.8",
    "lodash": "4.17.20",
    "moment": "2.27.0",
    "node-plop": "0.25.0",
    "plop": "2.6.0",
    "prettier": "2.0.1",
    "querystring-es3": "0.2.1",
    "react": "^17.0.2",
    "react-app-polyfill": "1.0.6",
    "react-beautiful-dnd": "13.1.0",
    "react-dom": "^17.0.2",
    "react-helmet-async": "1.0.4",
    "react-i18next": "11.3.4",
    "react-redux": "7.2.4",
    "react-router-dom": "5.1.2",
    "react-scripts": "5.0.0",
    "react-test-renderer": "16.13.0",
    "react-use": "17.2.4",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "sanitize.css": "11.0.0",
    "sass": "1.55.0",
    "serve": "11.3.0",
    "shelljs": "0.8.3",
    "styled-components": "5.0.1",
    "stylelint": "13.2.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "8.8.2",
    "typescript": "4.9.5",
    "universal-cookie": "4.0.4",
    "use-merge-value": "1.0.2",
    "@noinghe/shared": "*"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "yarn build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn eslint -- src",
    "lint:fix": "yarn eslint -- --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "generate:type-server": "npx @manifoldco/swagger-to-ts http://127.0.0.1:8000/docs/swagger/?format=.json --output src/types/generate/index.ts",
    "generate-graphql:local": "graphql-codegen --config ./src/graphql/codegen/config-local.yml",
    "codegen": "npm run generate-graphql:local",
    "start:dev": "npm run codegen && npm run start",
    "dev": "npm run start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint -- --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "@types/lodash": "4.14.160",
    "@types/react-beautiful-dnd": "13.1.0",
    "typescript-plugin-css-modules": "2.4.0"
  }
}
