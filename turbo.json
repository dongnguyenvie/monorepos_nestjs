{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**",
        ".svelte-kit/**",
        ".vercel/**",
        "$houdini/**"
      ],
      "cache": true
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
